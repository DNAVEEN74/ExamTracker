{
  "colors": {
    "background": {
      "primary": "#FFFFFF",
      "secondary": "#F8F9FA",
      "tertiary": "#F1F3F5"
    },
    "text": {
      "primary": "#1A1A1A",
      "secondary": "#5C5C5C",
      "tertiary": "#8C8C8C",
      "disabled": "#B8B8B8"
    },
    "accent": {
      "primary": "#FF6B35",
      "secondary": "#FF8555",
      "light": "#FFF5F2",
      "dark": "#E65A2E"
    },
    "success": {
      "primary": "#10B981",
      "light": "#D1FAE5",
      "dark": "#059669"
    },
    "warning": {
      "primary": "#F59E0B",
      "light": "#FEF3C7",
      "dark": "#D97706"
    },
    "error": {
      "primary": "#EF4444",
      "light": "#FEE2E2",
      "dark": "#DC2626"
    },
    "info": {
      "primary": "#3B82F6",
      "light": "#DBEAFE",
      "dark": "#2563EB"
    },
    "border": {
      "default": "#E5E7EB",
      "light": "#F3F4F6",
      "dark": "#D1D5DB"
    },
    "ad": {
      "background": "#FAFBFC",
      "border": "#E8EAED",
      "label": "#9CA3AF"
    }
  },
  "typography": {
    "fontFamily": {
      "primary": "Inter, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
      "mono": "'Courier New', monospace"
    },
    "fontSize": {
      "xs": "12px",
      "sm": "14px",
      "base": "16px",
      "lg": "18px",
      "xl": "20px",
      "2xl": "24px",
      "3xl": "30px",
      "4xl": "36px"
    },
    "fontWeight": {
      "regular": 400,
      "medium": 500,
      "semibold": 600,
      "bold": 700
    },
    "lineHeight": {
      "tight": 1.2,
      "snug": 1.3,
      "normal": 1.4,
      "relaxed": 1.5,
      "loose": 1.6
    }
  },
  "spacing": {
    "xs": "4px",
    "sm": "8px",
    "md": "16px",
    "lg": "24px",
    "xl": "32px",
    "2xl": "48px",
    "3xl": "64px"
  },
  "borderRadius": {
    "sm": "4px",
    "md": "8px",
    "lg": "12px",
    "xl": "16px",
    "full": "9999px"
  },
  "shadows": {
    "none": "none",
    "sm": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
    "base": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px -1px rgba(0, 0, 0, 0.1)",
    "md": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -2px rgba(0, 0, 0, 0.1)",
    "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -4px rgba(0, 0, 0, 0.1)",
    "xl": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 8px 10px -6px rgba(0, 0, 0, 0.1)",
    "cardHover": "0 4px 12px rgba(0, 0, 0, 0.08)",
    "accentCta": "0 2px 8px rgba(255, 107, 53, 0.24)"
  },
  "breakpoints": {
    "mobile": "320px",
    "tablet": "768px",
    "desktop": "1024px",
    "wide": "1440px"
  },
  "zIndex": {
    "base": 0,
    "dropdown": 1000,
    "sticky": 1020,
    "fixed": 1030,
    "modalBackdrop": 1040,
    "modal": 1050,
    "popover": 1060,
    "tooltip": 1070
  },
  "transitions": {
    "fast": "150ms ease-in-out",
    "base": "200ms ease-in-out",
    "slow": "300ms ease-in-out"
  },
  "components": {
    "examCard": {
      "background": "#F8F9FA",
      "border": "#E5E7EB",
      "borderRadius": "12px",
      "padding": "16px",
      "marginBottom": "16px",
      "hover": {
        "borderColor": "#FF6B35",
        "borderWidth": "2px",
        "shadow": "0 4px 12px rgba(0, 0, 0, 0.08)"
      },
      "newIndicator": {
        "color": "#FF6B35",
        "width": "4px"
      }
    },
    "button": {
      "height": "44px",
      "borderRadius": "8px",
      "fontSize": "16px",
      "fontWeight": 600,
      "transition": "200ms ease-in-out",
      "primary": {
        "background": "#FF6B35",
        "color": "#FFFFFF",
        "hoverBackground": "#E65A2E",
        "shadow": "0 2px 8px rgba(255, 107, 53, 0.24)"
      },
      "secondary": {
        "background": "transparent",
        "color": "#1A1A1A",
        "border": "1px solid #D1D5DB",
        "hoverBackground": "#F8F9FA",
        "hoverBorder": "#8C8C8C"
      },
      "ghost": {
        "background": "transparent",
        "color": "#5C5C5C",
        "hoverColor": "#1A1A1A",
        "hoverTextDecoration": "underline"
      },
      "disabled": {
        "background": "#F1F3F5",
        "color": "#B8B8B8",
        "border": "1px solid #E5E7EB",
        "cursor": "not-allowed"
      }
    },
    "input": {
      "height": "56px",
      "borderRadius": "8px",
      "border": "2px solid #D1D5DB",
      "fontSize": "18px",
      "padding": "16px",
      "focus": {
        "borderColor": "#FF6B35",
        "outline": "none"
      },
      "error": {
        "borderColor": "#EF4444"
      }
    },
    "deadlineBadge": {
      "fontSize": "14px",
      "fontWeight": 500,
      "padding": "4px 8px",
      "borderRadius": "9999px",
      "safe": {
        "background": "#F1F3F5",
        "color": "#8C8C8C"
      },
      "warning": {
        "background": "#FFF5F2",
        "color": "#FF6B35"
      },
      "urgent": {
        "background": "#FEE2E2",
        "color": "#EF4444",
        "animation": "pulse 0.8s ease-in-out infinite"
      }
    },
    "eligibilityStatus": {
      "fontSize": "16px",
      "fontWeight": 500,
      "display": "inline-flex",
      "alignItems": "center",
      "gap": "4px",
      "eligible": {
        "color": "#10B981",
        "icon": "✓"
      },
      "notEligible": {
        "color": "#EF4444",
        "icon": "✗"
      },
      "partial": {
        "color": "#F59E0B",
        "icon": "⚠"
      }
    },
    "header": {
      "height": "64px",
      "background": "#FFFFFF",
      "borderBottom": "1px solid #E5E7EB",
      "padding": "0 24px",
      "position": "sticky",
      "top": 0,
      "zIndex": 1020
    },
    "nativeAd": {
      "background": "#FAFBFC",
      "border": "1px solid #E8EAED",
      "borderRadius": "12px",
      "padding": "16px",
      "label": {
        "fontSize": "12px",
        "color": "#9CA3AF",
        "textTransform": "uppercase",
        "letterSpacing": "0.5px"
      }
    }
  },
  "accessibility": {
    "minTouchTarget": "44px",
    "focusOutline": "2px solid #FF6B35",
    "focusOutlineOffset": "2px"
  },
  "performance": {
    "firstContentfulPaint": "1.8s",
    "timeToInteractive": "3.5s",
    "totalPageSize": "500KB",
    "lighthouseScore": 90
  }
}
