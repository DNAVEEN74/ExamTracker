{
  "name": "examtracker-scraper",
  "version": "1.0.0",
  "description": "Official Indian government recruitment site scraper for ExamTracker India",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "scrape": "node index.js",
    "scrape:p0": "PRIORITY_FILTER=P0 node index.js",
    "scrape:p0p1": "PRIORITY_FILTER=P0,P1 node index.js",
    "scrape:all": "PRIORITY_FILTER=P0,P1,P2,P3 node index.js",
    "dev": "LOG_LEVEL=debug PRIORITY_FILTER=P0 node index.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.500.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0",
    "p-limit": "^5.0.0",
    "pino": "^8.17.0",
    "playwright": "^1.40.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
